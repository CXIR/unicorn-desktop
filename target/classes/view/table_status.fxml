<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Table_status">
    <children>
        <VBox prefHeight="267.0" prefWidth="388.0" spacing="30.0">
            <children>
                <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0">
                    <children>
                        <TextField fx:id="txt" />
                        <Button mnemonicParsing="false" onAction="#search" text="Rechercher" />
                        <Button mnemonicParsing="false" onAction="#cancel" text="Annuler" />
                    </children>
                </HBox>
                <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
                    <columns>
                        <TableColumn fx:id="name" prefWidth="168.0" text="Nom" />
                    </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
                </TableView>
            </children>
        </VBox>
    </children>
</Group>
