<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.geometry.Insets?>
<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Table_vehicle">
    <children>
        <VBox maxHeight="500.0" maxWidth="750.0" spacing="30.0">
            <children>
                <HBox spacing="30.0">
                    <children>
                        <TextField />
                        <Button mnemonicParsing="false" text="Rechercher" />
                    </children>
                    <padding>
                        <Insets left="5.0" top="30.0" />
                    </padding>
                </HBox>
                <TableView fx:id="table" prefWidth="750">
                    <columns>
                        <TableColumn fx:id="user" prefWidth="-1" text="Nom" />
                        <TableColumn id="col" fx:id="brand" editable="false"  prefWidth="-1.0" text="Marque" />
                        <TableColumn id="col" fx:id="model" editable="false" prefWidth="-1.0" text="Modèle" />
                        <TableColumn id="col" fx:id="registr" editable="false" minWidth="30" prefWidth="-1.0" text="N° d'enregistrement" />
                        <TableColumn id="col" fx:id="place" editable="false" minWidth="20" prefWidth="-1.0" text="Nombre de places" />
                        <TableColumn id="col" fx:id="type" editable="false" prefWidth="-1.0" text="Type de Véhicule" />
                        <TableColumn id="col" fx:id="valid" editable="true" prefWidth="-1.0" text="Validé" />
                    </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
                </TableView>
            </children>
        </VBox>
    </children>
</Group>
