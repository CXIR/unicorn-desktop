<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="User">
   <children>
      <VBox prefHeight="200.0" prefWidth="200.0" spacing="50.0">
         <children>
            <Button fx:id="btn1" mnemonicParsing="false" onAction="#newUser" prefHeight="40.0" prefWidth="210.0" text="Ajouter un utilisateur" />
            <Button fx:id="btn2" mnemonicParsing="false" onAction="#newUsers" prefHeight="40.0" prefWidth="210.0" text="Ajouter des utilisateurs" />
            <Button fx:id="btn3" mnemonicParsing="false" onAction="#search" prefHeight="40.0" prefWidth="248.0" text="Rechercher un utilisateur" />
         </children>
      </VBox>
   </children>
</Group>
