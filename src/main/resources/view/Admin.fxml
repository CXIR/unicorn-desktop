<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<Group xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Admin">
   <children>
      <VBox alignment="CENTER" spacing="50.0">
         <children>
            <Button fx:id="btn1" mnemonicParsing="false" onAction="#newAdmin" prefHeight="40.0" text="Ajouter un administrateur" textAlignment="CENTER" />
            <Button fx:id="btn2" mnemonicParsing="false" onAction="#search" prefHeight="40.0" text="Rechercher un administrateur" textAlignment="CENTER" />
         </children>
      </VBox>
   </children>
</Group>
