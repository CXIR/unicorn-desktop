<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<Group xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.AddAdmin">
   <children>
      <VBox prefHeight="267.0" prefWidth="388.0" spacing="30.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="30.0">
               <children>
                  <TextField fx:id="txt" />
                  <Button mnemonicParsing="false" onAction="#search" text="Rechercher" />
                  <Button mnemonicParsing="false" onAction="#cancel" text="Annuler" />
               </children>
            </HBox>
            <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
              <columns>
                <TableColumn fx:id="name" prefWidth="168.0" text="Name" />
                <TableColumn id="col" fx:id="admin" prefWidth="105.0" text="Admin" />
                  <TableColumn id="col" fx:id="superAd" prefWidth="114.0" text="Super Admin" />
              </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</Group>
