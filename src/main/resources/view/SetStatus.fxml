<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SetStatus">
    <children>
        <VBox spacing="50.0">
            <children>
                <GridPane hgap="5.0" vgap="30.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="200.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <Label text="Nom du Status :" />
                        <TextField fx:id="nameAd" GridPane.columnIndex="1" />
                        <Label fx:id="nameDi" text="name" visible="false" GridPane.columnIndex="1" />
                    </children>
                </GridPane>
                <HBox alignment="CENTER" spacing="50.0">
                    <children>
                        <Button fx:id="valid" mnemonicParsing="false" onAction="#validate" text="Valider" />
                        <Button fx:id="cancel" mnemonicParsing="false" onAction="#back" text="Annuler" />
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>
</Group>
