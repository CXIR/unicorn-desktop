<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<Group xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Plugins">
    <children>
        <VBox fx:id="box" alignment="CENTER" spacing="50.0">
            <children>
            <TableView fx:id="table">
              <columns>
                <TableColumn fx:id="col" prefWidth="75.0" text="Nom du plugins" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
            <HBox spacing="30.0">
               <children>
                      <Button fx:id="btn1" mnemonicParsing="false" onAction="#add" prefHeight="40.0" text="Ajouter un plugin" />
                      <Button fx:id="btn2" mnemonicParsing="false" onAction="#del" prefHeight="40.0" text="Supprmer un plugin" />
               </children>
            </HBox>
            </children>
        </VBox>
    </children>
</Group>
