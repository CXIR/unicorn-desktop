<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<Group xmlns="http://javafx.com/javafx/8.0.112-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Site">
   <children>
      <VBox alignment="CENTER" spacing="50.0">
         <children>
            <Button mnemonicParsing="false" onAction="#newSite" prefHeight="40.0" text="Ajouter un site" />
            <Button mnemonicParsing="false" onAction="#search" prefHeight="40.0" text="Rechercher un site" />
         </children>
      </VBox>
   </children>
</Group>
